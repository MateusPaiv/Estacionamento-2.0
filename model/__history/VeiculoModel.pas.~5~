unit VeiculoModel;

interface
type
  TVeiculo = class
    private
      Fmodelo: string;
      Fplaca: string;
      Fano: string;
    public
      property modelo: string read Fmodelo write Fmodelo;
      property placa: string read Fplaca write Fplaca;
      property ano: string read Fano write Fano;
      constructor Create(const modelo,placa,ano:string);
  end;
implementation

{ TVeiculo }

constructor TVeiculo.Create(const modelo, placa, ano: string);
begin
   Fmodelo:=modelo;
   Fplaca:=placa;
   Fano:=ano;
end;
end.
